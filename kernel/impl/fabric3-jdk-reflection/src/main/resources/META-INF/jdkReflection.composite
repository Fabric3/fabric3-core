<?xml version="1.0" encoding="UTF-8"?>
<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:f3="urn:fabric3.org"
           targetNamespace="urn:fabric3.org"
           name="JDKReflectionComposite"
           autowire="true">

    <component name="JDKInstantiatorFactory">
        <f3:implementation.system class="org.fabric3.implementation.reflection.jdk.JDKInstantiatorFactory"/>
    </component>


    <component name="JDKInjectorFactory">
        <f3:implementation.system class="org.fabric3.implementation.reflection.jdk.JDKInjectorFactory"/>
    </component>


    <component name="JDKLifecycleInvokerFactory">
        <f3:implementation.system class="org.fabric3.implementation.reflection.jdk.JDKLifecycleInvokerFactory"/>
    </component>

    <component name="JDKServiceInvokerFactory">
        <f3:implementation.system class="org.fabric3.implementation.reflection.jdk.JDKServiceInvokerFactory"/>
    </component>

    <component name="JDKConsumerInvokerFactory">
        <f3:implementation.system class="org.fabric3.implementation.reflection.jdk.JDKConsumerInvokerFactory"/>
    </component>

</composite>
