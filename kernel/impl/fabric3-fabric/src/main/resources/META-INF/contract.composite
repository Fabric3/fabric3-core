<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:f3="urn:fabric3.org"
           targetNamespace="urn:fabric3.org"
           name="ContractComposite"
           autowire="true">
    <component name="ContractMatcher">
        <f3:implementation.system class="org.fabric3.fabric.contract.DefaultContractMatcher"/>
    </component>

    <component name="JavaContractMatcherExtension">
        <f3:implementation.system class="org.fabric3.fabric.contract.JavaContractMatcherExtension"/>
    </component>

    <component name="RemoteToJavaContractMatcherExtension">
        <f3:implementation.system class="org.fabric3.fabric.contract.RemoteToJavaContractMatcherExtension"/>
    </component>

    <component name="JavaToRemoteContractMatcherExtension">
        <f3:implementation.system class="org.fabric3.fabric.contract.JavaToRemoteContractMatcherExtension"/>
    </component>

</composite>