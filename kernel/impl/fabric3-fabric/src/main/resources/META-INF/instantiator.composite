<?xml version="1.0" encoding="UTF-8"?>
<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:f3="urn:fabric3.org"
           targetNamespace="urn:fabric3.org"
           name="InstantiatorComposite"
           autowire="true">

    <component name="LogicalModelInstantiator">
        <f3:implementation.system class="org.fabric3.fabric.domain.instantiator.LogicalModelInstantiatorImpl"/>
    </component>

    <component name="AtomicComponentInstantiator">
        <f3:implementation.system class="org.fabric3.fabric.domain.instantiator.component.AtomicComponentInstantiatorImpl"/>
    </component>

    <component name="CompositeComponentInstantiator">
        <f3:implementation.system class="org.fabric3.fabric.domain.instantiator.component.CompositeComponentInstantiatorImpl"/>
    </component>

    <component name="WireInstantiator">
        <f3:implementation.system class="org.fabric3.fabric.domain.instantiator.wire.WireInstantiatorImpl"/>
    </component>

    <component name="ChannelInstantiator">
        <f3:implementation.system class="org.fabric3.fabric.domain.instantiator.channel.ChannelInstantiatorImpl"/>
    </component>

    <component name="PromotionNormalizer">
        <f3:implementation.system class="org.fabric3.fabric.domain.instantiator.promotion.PromotionNormalizerImpl"/>
    </component>

    <component name="AutowireNormalizer">
        <f3:implementation.system class="org.fabric3.fabric.domain.instantiator.component.AutowireNormalizerImpl"/>
    </component>

    <component name="PromotionResolutionService">
        <f3:implementation.system class="org.fabric3.fabric.domain.instantiator.promotion.PromotionResolutionServiceImpl"/>
    </component>

    <component name="TypeAutowireResolver">
        <f3:implementation.system class="org.fabric3.fabric.domain.instantiator.wire.TypeAutowireResolver"/>
    </component>

    <component name="AutowireInstantiator">
        <f3:implementation.system class="org.fabric3.fabric.domain.instantiator.wire.AutowireInstantiatorImpl"/>
    </component>

    <component name="DocumentLoader">
        <f3:implementation.system class="org.fabric3.fabric.xml.DocumentLoaderImpl"/>
    </component>

    <component name="Collector">
        <f3:implementation.system class="org.fabric3.fabric.domain.collector.CollectorImpl"/>
    </component>

</composite>
