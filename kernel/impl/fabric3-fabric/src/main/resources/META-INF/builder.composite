<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:f3-impl="urn:fabric3.org:implementation"
           xmlns:f3-core="urn:fabric3.org:core"
           targetNamespace="urn:maven:org.codehaus.fabric3:boot"
           name="BuilderComposite"
           autowire="true">

    <component name="Connector">
        <f3-impl:implementation.system class="org.fabric3.fabric.builder.ConnectorImpl"/>
    </component>

    <component name="ChannelConnector">
        <f3-impl:implementation.system class="org.fabric3.fabric.builder.ChannelConnectorImpl"/>
    </component>

    <component name="ClassLoaderBuilder">
        <f3-impl:implementation.system class="org.fabric3.fabric.builder.classloader.ClassLoaderBuilderImpl"/>
    </component>

    <component name="ClassLoaderTracker">
        <f3-impl:implementation.system class="org.fabric3.fabric.builder.classloader.ClassLoaderTrackerImpl"/>
    </component>

    <component name="ClassLoaderWireBuilder">
        <f3-impl:implementation.system class="org.fabric3.fabric.builder.classloader.ClassLoaderWireBuilderImpl"/>
    </component>

    <component name="TransformerInterceptorFactory">
        <f3-impl:implementation.system class="org.fabric3.fabric.builder.transform.TransformerInterceptorFactoryImpl"/>
    </component>

    <component name="ChannelSourceAttacher" f3-core:key="org.fabric3.fabric.model.physical.ChannelSourceDefinition">
        <f3-impl:implementation.system class="org.fabric3.fabric.builder.channel.ChannelSourceAttacher"/>
    </component>

    <component name="ChannelTargetAttacher" f3-core:key="org.fabric3.fabric.model.physical.ChannelTargetDefinition">
        <f3-impl:implementation.system class="org.fabric3.fabric.builder.channel.ChannelTargetAttacher"/>
    </component>

    <component name="TypeEventFilterBuilder" f3-core:key="org.fabric3.fabric.model.physical.TypeEventFilterDefinition">
        <f3-impl:implementation.system class="org.fabric3.fabric.builder.channel.TypeEventFilterBuilder"/>
    </component>

</composite>