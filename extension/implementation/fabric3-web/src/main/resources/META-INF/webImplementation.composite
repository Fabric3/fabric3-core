<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:sca="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:f3-core="urn:fabric3.org:core"
           xmlns:f3-impl="urn:fabric3.org:implementation"
           targetNamespace="urn:fabric3.org:implementation"
           name="WebImplementationExtension"
           autowire="true">

    <component name="WebComponentLoader">
        <f3-impl:implementation.system class="org.fabric3.implementation.web.introspection.WebComponentLoader"/>
    </component>

    <component name="WebImplementationProcessor">
        <f3-impl:implementation.system class="org.fabric3.implementation.web.introspection.WebImplementationIntrospectorImpl"/>
    </component>

    <component name="WebXmlIntrospector">
        <f3-impl:implementation.system class="org.fabric3.implementation.web.introspection.WebXmlIntrospectorImpl"/>
    </component>

    <component name="WebComponentGenerator" f3-core:key="org.fabric3.implementation.web.model.WebImplementation">
        <f3-impl:implementation.system class="org.fabric3.implementation.web.generator.WebComponentGenerator"/>
    </component>

    <component name="WebComponentBuilder" f3-core:key="org.fabric3.implementation.web.provision.WebComponentDefinition">
        <f3-impl:implementation.system class="org.fabric3.implementation.web.runtime.WebComponentBuilder"/>
    </component>
    <component name="InjectionFactory">
        <f3-impl:implementation.system class="org.fabric3.implementation.web.runtime.InjectorFactoryImpl"/>
    </component>

    <component name="WebSourceWireAttacher" f3-core:key="org.fabric3.implementation.web.provision.WebComponentSourceDefinition">
        <f3-impl:implementation.system class="org.fabric3.implementation.web.runtime.WebComponentSourceWireAttacher"/>
    </component>
    
</composite>
