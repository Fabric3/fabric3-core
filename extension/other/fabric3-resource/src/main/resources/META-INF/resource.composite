<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:f3-core="urn:fabric3.org:core"
           xmlns:f3-impl="urn:fabric3.org:implementation"
           targetNamespace="urn:fabric3.org:other"
           name="ResourceExtension"
           autowire="true">

    <component name="ResourceProcessor" f3-core:key="org.fabric3.api.annotation.Resource">
        <f3-impl:implementation.system class="org.fabric3.resource.introspection.ResourceProcessor"/>
    </component>

    <component name="JSR250ResourceProcessor" f3-core:key="javax.annotation.Resource">
        <f3-impl:implementation.system class="org.fabric3.resource.introspection.JSR250ResourceProcessor"/>
    </component>

    <component name="DataSourceTypeHandler" f3-core:key="javax.sql.DataSource">
        <f3-impl:implementation.system class="org.fabric3.resource.introspection.DataSourceTypeHandler"/>
    </component>

    <component name="JSR250DataSourceTypeHandler" f3-core:key="javax.sql.DataSource">
        <f3-impl:implementation.system class="org.fabric3.resource.introspection.JSR250DataSourceTypeHandler"/>
    </component>

    <component name="SystemSourcedResourceWireGenerator" f3-core:key="org.fabric3.resource.model.SystemSourcedResourceReference">
        <f3-impl:implementation.system class="org.fabric3.resource.generator.SystemSourcedResourceReferenceGenerator"/>
    </component>

    <component name="DataSourceGenerator" f3-core:key="org.fabric3.resource.model.DataSourceResourceReference">
        <f3-impl:implementation.system class="org.fabric3.resource.generator.DataSourceResourceReferenceGenerator"/>
    </component>

    <component name="SystemSourcedResourceWireAttacher" f3-core:key="org.fabric3.resource.provision.SystemSourcedTargetDefinition">
        <f3-impl:implementation.system class="org.fabric3.resource.runtime.wire.SystemSourcedResourceWireAttacher"/>
    </component>

    <component name="DataSourceWireAttacher" f3-core:key="org.fabric3.resource.provision.DataSourceTargetDefinition">
        <f3-impl:implementation.system class="org.fabric3.resource.runtime.wire.DataSourceWireAttacher"/>
    </component>

    <component name="DataSourceRegistry">
        <f3-impl:implementation.system class="org.fabric3.resource.runtime.ds.DataSourceRegistryImpl"/>
    </component>

</composite>