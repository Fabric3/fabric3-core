<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:f3="urn:fabric3.org"
           targetNamespace="urn:fabric3.org"
           name="RsJerseyExtension"
           autowire="true">


    <component name="RsContainerManager">
        <f3:implementation.system class="org.fabric3.binding.rs.runtime.container.RsContainerManagerImpl"/>
    </component>

    <component name="FilterRegistry">
        <f3:implementation.system class="org.fabric3.binding.rs.runtime.filter.FilterRegistryImpl"/>
    </component>

    <component name="SubclassGenerator">
        <f3:implementation.system class="org.fabric3.binding.rs.runtime.bytecode.SubclassGeneratorImpl"/>
    </component>

    <component name="NameBindingFilterProvider">
        <f3:implementation.system class="org.fabric3.binding.rs.runtime.filter.NameBindingFilterProviderImpl"/>
    </component>

    <component name="RsSourceWireAttacher" f3:key="org.fabric3.binding.rs.provision.RsSourceDefinition">
        <f3:implementation.system class="org.fabric3.binding.rs.runtime.RsSourceWireAttacher"/>
        <property name="logLevel" source="$systemConfig/f3:binding.rs/@log.level"/>
    </component>

    <component name="ProviderBuilder" f3:key="org.fabric3.binding.rs.provision.PhysicalProviderResourceDefinition">
        <f3:implementation.system class="org.fabric3.binding.rs.runtime.builder.ProviderBuilder"/>
    </component>

    <component name="RsTargetWireAttacher" f3:key="org.fabric3.binding.rs.provision.RsTargetDefinition">
        <f3:implementation.system class="org.fabric3.binding.rs.runtime.RsTargetWireAttacher"/>
    </component>

</composite>
