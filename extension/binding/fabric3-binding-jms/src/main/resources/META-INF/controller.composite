<?xml version="1.0" encoding="UTF-8"?>
<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:f3="urn:fabric3.org"
           xmlns:sca="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           targetNamespace="urn:fabric3.org"
           name="JmsControllerExtension">

    <component f3:key="sca:binding.jms" name="JMSBindingLoader">
        <f3:implementation.system class="org.fabric3.binding.jms.introspection.JmsBindingLoader"/>
    </component>

    <component name="JmsWireBindingGenerator" f3:key="org.fabric3.api.binding.jms.model.JmsBindingDefinition">
        <f3:implementation.system class="org.fabric3.binding.jms.generator.JmsWireBindingGenerator"/>
    </component>

    <component name="JmsBindingPostProcessor">
        <f3:implementation.system class="org.fabric3.binding.jms.introspection.JmsBindingPostProcessor"/>
    </component>

    <component name="JmsConnectionBindingGenerator" f3:key="org.fabric3.api.binding.jms.model.JmsBindingDefinition">
        <f3:implementation.system class="org.fabric3.binding.jms.generator.JmsConnectionBindingGenerator"/>
    </component>

    <component name="MessageTypeIntrospector">
        <f3:implementation.system class="org.fabric3.binding.jms.generator.PayloadTypeIntrospectorImpl"/>
    </component>

    <component name="ConnectionFactoryResourceGenerator" f3:key="org.fabric3.api.binding.jms.resource.ConnectionFactoryResource">
        <f3:implementation.system class="org.fabric3.binding.jms.generator.ConnectionFactoryResourceGenerator"/>
    </component>

    <component name="ConnectionFactoryResourceLoader" f3:key="f3:connection.factory">
        <f3:implementation.system class="org.fabric3.binding.jms.introspection.ConnectionFactoryResourceLoader"/>
    </component>


</composite>
