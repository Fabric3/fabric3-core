<?xml version="1.0" encoding="UTF-8"?>
<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:f3="urn:fabric3.org"
           targetNamespace="urn:fabric3.org"
           name="MetroExtension"
           autowire="true">

    <component name="EndpointSynthesizer">
        <f3:implementation.system class="org.fabric3.binding.ws.metro.generator.java.EndpointSynthesizerImpl"/>
    </component>

    <component name="TargetUrlResolver">
        <f3:implementation.system class="org.fabric3.binding.ws.metro.generator.resolver.TargetUrlResolverImpl"/>
    </component>

    <component name="JavaInterfaceGenerator">
        <f3:implementation.system class="org.fabric3.binding.ws.metro.generator.java.codegen.InterfaceGeneratorImpl"/>
    </component>

    <component name="MetroWireBindingGenerator" f3:key="org.fabric3.api.binding.ws.model.WsBindingDefinition">
        <f3:implementation.system class="org.fabric3.binding.ws.metro.generator.MetroWireBindingGenerator"/>
    </component>

    <component name="JavaGeneratorDelegate" f3:key="org.fabric3.spi.model.type.java.JavaServiceContract">
        <f3:implementation.system class="org.fabric3.binding.ws.metro.generator.java.JavaGeneratorDelegate"/>
    </component>

    <component name="MetroContributionServiceListener">
        <f3:implementation.system class="org.fabric3.binding.ws.metro.contribution.MetroContributionServiceListener"/>
    </component>


    <component f3:key="org.fabric3.binding.ws.metro.provision.MetroJavaWireSourceDefinition" name="MetroJavaSourceWireAttacher">
        <f3:implementation.system class="org.fabric3.binding.ws.metro.runtime.wire.MetroJavaSourceWireAttacher"/>
    </component>

    <component f3:key="org.fabric3.binding.ws.metro.provision.MetroJavaWireTargetDefinition" name="MetroJavaTargetWireAttacher">
        <f3:implementation.system class="org.fabric3.binding.ws.metro.runtime.wire.MetroJavaTargetWireAttacher"/>
        <reference name="executorService" target="RuntimeThreadPoolExecutor"/>
    </component>

    <component name="WireAttacherHelper">
        <f3:implementation.system class="org.fabric3.binding.ws.metro.runtime.wire.WireAttacherHelperImpl"/>
    </component>

    <component name="EndpointService">
        <f3:implementation.system class="org.fabric3.binding.ws.metro.runtime.core.EndpointServiceImpl"/>
        <reference name="executorService" target="RuntimeThreadPoolExecutor"/>
    </component>

</composite>
