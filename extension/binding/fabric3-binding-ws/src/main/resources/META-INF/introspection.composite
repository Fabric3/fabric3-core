<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:sca="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:f3="urn:fabric3.org"
           xmlns:wsa="http://www.w3.org/2005/08/addressing"
           targetNamespace="urn:fabric3.org"
           name="introspection">

    <component name="WsBindingLoader" f3:key="sca:binding.ws">
        <f3:implementation.system class="org.fabric3.binding.ws.loader.WsBindingLoader"/>
    </component>

    <component name="WsaLoader" f3:key="wsa:EndpointReference">
        <f3:implementation.system class="org.fabric3.binding.ws.loader.WsaLoader"/>
    </component>

    <component name="WsCallbackBindingGenerator" f3:key="org.fabric3.binding.ws.api.model.WsBindingDefinition">
        <f3:implementation.system class="org.fabric3.binding.ws.generator.WsCallbackBindingGenerator"/>
    </component>

</composite>
