<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:f3="urn:fabric3.org"
           targetNamespace="urn:fabric3.org"
           name="RsExtension"
           autowire="true">

    <component name="RsBindingLoader" f3:key="f3:binding.rs">
        <f3:implementation.system class="org.fabric3.binding.rs.introspection.RsBindingLoader"/>
    </component>

    <component name="RsPostProcessor">
        <f3:implementation.system class="org.fabric3.binding.rs.introspection.RsPostProcessor"/>
    </component>

    <component name="RsBindingGenerator" f3:key="org.fabric3.api.binding.rs.RsBindingDefinition">
