<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:f3-core="urn:fabric3.org:core"
           xmlns:f3-impl="urn:fabric3.org:implementation"
           xmlns:f3-binding="urn:org.fabric3:binding"
           targetNamespace="urn:fabric3.org:implementation"
           name="RsExtension"
           autowire="true">

    <component name="RsBindingLoader" f3-core:key="f3-binding:binding.rs">
        <f3-impl:implementation.system class="org.fabric3.binding.rs.introspection.RsBindingLoader"/>
    </component>

    <!-- Rs Binding Generator -->
    <component name="RsBindingGenerator" f3-core:key="org.fabric3.binding.rs.model.RsBindingDefinition">
        <f3-impl:implementation.system class="org.fabric3.binding.rs.generator.RsBindingGenerator"/>
    </component>

    <!-- Rs Binding Wire Attachers  -->
    <component name="RsSourceWireAttacher" f3-core:key="org.fabric3.binding.rs.provision.RsSourceDefinition">
        <f3-impl:implementation.system class="org.fabric3.binding.rs.runtime.RsSourceWireAttacher"/>
    </component>


</composite>
