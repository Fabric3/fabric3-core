<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:f3-impl="urn:fabric3.org:implementation"
           targetNamespace="urn:fabric3.org:federation"
           name="FederationParticipantDeploymentExtension"
           autowire="true">

    <component name="ZoneDeploymentCommandExecutor">
        <f3-impl:implementation.system class="org.fabric3.federation.deployment.executor.DeploymentCommandExecutor"/>
    </component>

    <component name="RuntimeUpdateCommandExecutor">
        <f3-impl:implementation.system class="org.fabric3.federation.deployment.executor.RuntimeUpdateCommandExecutor"/>
    </component>

    <component name="CommitCommandExecutor">
        <f3-impl:implementation.system class="org.fabric3.federation.deployment.executor.CommitCommandExecutor"/>
    </component>

    <component name="RollbackCommandExecutor">
        <f3-impl:implementation.system class="org.fabric3.federation.deployment.executor.RollbackCommandExecutor"/>
    </component>

    <component name="DeploymentCache">
        <f3-impl:implementation.system class="org.fabric3.federation.deployment.cache.DeploymentCacheImpl"/>
    </component>


</composite>
