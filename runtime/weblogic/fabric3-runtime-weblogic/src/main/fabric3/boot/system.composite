<?xml version="1.0" encoding="UTF-8"?>
<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:f3="urn:fabric3.org"
           xmlns:introspection="urn:fabric3.org"
           name="WebLogicSystemComposite"
           autowire="true">

    <include name="ContributionComposite" scdlResource="META-INF/contribution.composite"/>

    <include name="FabricComposite" scdlResource="META-INF/fabric.composite"/>

    <include name="JavaIntrospectionComposite" scdlResource="META-INF/javaBootIntrospection.composite"/>

    <include name="XMLIntrospectionComposite" scdlResource="META-INF/xmlIntrospection.composite"/>

    <include name="PojoComposite" scdlResource="META-INF/pojoImplementation.composite"/>

    <include name="JDKReflectionComposite" scdlResource="META-INF/jdkReflection.composite"/>

    <include name="PolicyComposite" scdlResource="META-INF/policy.composite"/>

    <include name="SystemImplementationComposite" scdlResource="META-INF/systemImplementation.composite"/>

    <include name="SingletonImplementationComposite" scdlResource="META-INF/singletonImplementation.composite"/>

    <include name="MonitorComposite" scdlResource="META-INF/monitor.composite"/>

    <include name="TransformComposite" scdlResource="META-INF/transform.composite"/>

    <component name="FSArtifactCache">
        <f3:implementation.system class="org.fabric3.fabric.repository.FSArtifactCache"/>
    </component>

    <component name="SyntheticDirectoryContributionProcessor">
        <f3:implementation.system class="org.fabric3.contribution.archive.SyntheticDirectoryContributionProcessor"/>
    </component>

    <component name="SyntheticDirectoryClasspathProcessor">
        <f3:implementation.system class="org.fabric3.contribution.archive.SyntheticDirectoryClasspathProcessor"/>
    </component>

    <component name="SymLinkClasspathProcessor">
        <f3:implementation.system class="org.fabric3.contribution.processor.SymLinkClasspathProcessor"/>
    </component>
    
</composite>
