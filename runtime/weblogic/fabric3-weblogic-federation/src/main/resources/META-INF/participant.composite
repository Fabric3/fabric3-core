<?xml version="1.0" encoding="UTF-8"?>
<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:f3="urn:fabric3.org"
           targetNamespace="urn:fabric3.org"
           name="WebLogicFederationParticipantExtension"
           autowire="true">

    <component name="WebLogicParticipantTopologyService">
        <f3:implementation.system class="org.fabric3.runtime.weblogic.federation.WebLogicParticipantTopologyService"/>
        <property name="synchronize" source="$systemConfig//f3:federation/@synchronize"/>
        <property name="adminServerUrl" source="$systemConfig//f3:weblogic/@adminServerUrl"/>
    </component>


</composite>
