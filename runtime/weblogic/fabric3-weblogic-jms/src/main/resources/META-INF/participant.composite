<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:f3-core="urn:fabric3.org:core"
           xmlns:f3-impl="urn:fabric3.org:implementation"
           targetNamespace="urn:fabric3.org:weblogic"
           name="WebLogicJmsParticipantExtension"
           autowire="true">

    <component name="WebLogicConnectionFactoryManager">
        <f3-impl:implementation.system class="org.fabric3.runtime.weblogic.jms.runtime.WebLogicConnectionFactoryManager"/>
    </component>

    <component name="WebLogicConnectionFactoryResolver">
        <f3-impl:implementation.system class="org.fabric3.runtime.weblogic.jms.runtime.WebLogicConnectionFactoryResolver"/>
    </component>

    <component name="WebLogicDestinationResolver">
        <f3-impl:implementation.system class="org.fabric3.runtime.weblogic.jms.runtime.WebLogicDestinationResolver"/>
        <property name="adminServerUrl" source="$systemConfig//weblogic/@adminServerUrl"/>
    </component>


</composite>
