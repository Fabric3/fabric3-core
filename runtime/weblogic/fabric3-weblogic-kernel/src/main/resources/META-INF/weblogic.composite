<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:f3-core="urn:fabric3.org:core"
           xmlns:f3-impl="urn:fabric3.org:implementation"
           targetNamespace="urn:fabric3.org:weblogic"
           name="WeblogicExtensionComposite"
           autowire="true">

    <component name="WebLogicWorkScheduler">
        <f3-impl:implementation.system class="org.fabric3.runtime.weblogic.work.WebLogicWorkScheduler"/>
    </component>

    <component name="ServletRequestDispatcher">
        <f3-impl:implementation.system class="org.fabric3.runtime.weblogic.servlet.WebLogicServletHost"/>
    </component>

    <component name="WebLogicTransactionManager">
        <f3-impl:implementation.system class="org.fabric3.runtime.weblogic.tx.WebLogicTransactionManager"/>
    </component>

    <component name="DataSourceResolver">
        <f3-impl:implementation.system class="org.fabric3.runtime.weblogic.ds.DataSourceResolver"/>
    </component>

    <component name="WebLogicMonitorEventDispatcher">
        <f3-impl:implementation.system class="org.fabric3.runtime.weblogic.monitor.WebLogicMonitorEventDispatcher"/>
    </component>

</composite>
